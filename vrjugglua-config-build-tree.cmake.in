set(VRJUGGLUA_LIBRARIES vrjlua_buildtree_vrjugglua)
set(VRJUGGLUA_FLTK_CONSOLE_LIBRARIES vrjlua_buildtree_vrjugglua-fltk)

set(VRJUGGLUA_INCLUDE_DIRS
	"@CMAKE_CURRENT_SOURCE_DIR@"
	"@CMAKE_CURRENT_SOURCE_DIR@/Fl_Native_File_Chooser-0.86"
	"@CMAKE_CURRENT_SOURCE_DIR@/lua-5.1.4/src"    
	"@CMAKE_CURRENT_SOURCE_DIR@/luabind"  
	"@CMAKE_CURRENT_SOURCE_DIR@/osgLua/include"
	"@CMAKE_CURRENT_SOURCE_DIR@/osgLua/src")

