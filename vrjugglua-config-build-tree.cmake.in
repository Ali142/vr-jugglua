	get_filename_component(_IMPORT_PREFIX "${_IMPORT_PREFIX}" PATH)
endif()

set(VRJUGGLUA_INCLUDE_DIRS "${_IMPORT_PREFIX}/include")
set(_IMPORT_PREFIX)
